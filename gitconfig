[apply]
	whitespace = nowarn
[core]
	editor = mate -w
[merge]
	tool = opendiff
[push]
	default = matching
[color]
  ui = auto
[color "branch"]
	current = green
	local = white
	remote = cyan
[color "diff"]
	meta = yellow
	frag = magenta
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[color "interactive"]
  header = white bold
  prompt = green bold
  help = cyan
[alias]
	co = checkout
	d = dcommit
	b = branch
	re = rebase
	s = stash
	sub = submodule
