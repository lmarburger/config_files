[apply]
  whitespace = nowarn
[core]
  editor = mvim -f
  excludesfile = /Users/Larry/.gitignore
[merge]
  tool = opendiff
[push]
  default = current
[color]
  ui = auto
[color "branch"]
  current = green
  local = white
  remote = cyan
[color "diff"]
  meta = yellow
  frag = magenta
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[color "interactive"]
  header = white bold
  prompt = green bold
  help = cyan
[alias]
  a = add
  c = commit
  cl = clone
  co = checkout
  cp = cherry-pick
  d = dcommit
  b = branch
  re = rebase
  s = stash
  sub = submodule
  l = log --oneline --graph
[user]
  name = Larry Marburger
  email = larry@marburger.cc
